{
    "name": "brainstorm/perseo",
    "type": "library",
    "description": "A simple modular CMS optimized for SEO based on Slim v4",
    "keywords": [
        "Slim",
        "PerSeo",
        "PSR"
    ],
    "homepage": "https://github.com/BrainStormDevel/perseo",
    "license": "MIT",
    "require": {
        "slim/slim": ">=4.0",
        "slim/psr7": ">=1.5",
        "slim/http": ">=1.3",
        "nyholm/psr7": ">=1.4",
        "nyholm/psr7-server": ">=1.0",
        "laminas/laminas-diactoros": ">=2.5",
        "php-di/php-di": ">=6.4",
        "brainstormdevel/basepath": "^2.0",
        "slim/twig-view": ">=3.0",
        "catfan/medoo": ">=2.1",
        "composer/ca-bundle": ">=1.2",
        "odan/twig-assets": ">=3.2",
        "odan/session": ">=5.1",
        "monolog/monolog": ">=2.3",
        "ext-openssl": ">=7.2",
        "brainstorm/slim4locale": ">=1.0",
        "robmorgan/phinx": ">=0.13"
    },
	"autoload": {
        "psr-4": {
		"PerSeo\\": "Core/PerSeo",
		"Modules\\": "modules/"
        }
    },
	"config": {
		"optimize-autoloader": true
	}
}
