{
    "name": "brainstorm/perseo",
    "type": "library",
    "description": "A simple modular CMS optimized for SEO based on Slim v4",
    "keywords": [
        "Slim",
        "PerSeo",
        "PSR"
    ],
    "homepage": "https://github.com/BrainStormDevel/perseo",
    "license": "MIT",
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.3",
        "nyholm/psr7": "^1.3",
        "nyholm/psr7-server": "^1.0",
        "guzzlehttp/psr7": "^1.7",
        "http-interop/http-factory-guzzle": "^1.0",
        "laminas/laminas-diactoros": "^2.5",
        "php-di/php-di": "^6.3",
        "brainstormdevel/basepath": "^2.0",
        "slim/twig-view": "^3.0",
        "catfan/medoo": "^2.0",
        "composer/ca-bundle": "^1.2",
        "odan/twig-assets": "^3.2",
        "odan/session": "^5.1",
        "monolog/monolog": "^2.2",
        "ext-openssl": "^8.0",
        "brainstorm/slim4locale": "^1.0"
    },
	"autoload": {
        "psr-4": {
			"PerSeo\\": "Core/PerSeo",
			"Modules\\": "modules/"
        }
    },
	"config": {
		"optimize-autoloader": true
	}
}
