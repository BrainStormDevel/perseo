{
    "name": "per-seo/cms",
    "type": "project",
    "description": "A simple modular CMS optimized for SEO based on Slim v4",
    "keywords": [
        "Slim",
        "PerSeo",
        "PSR"
    ],
    "homepage": "https://github.com/per-seo/cms",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "slim/slim": ">=4.0",
        "slim/psr7": ">=1.5",
        "slim/http": ">=1.3",
        "nyholm/psr7": ">=1.4",
        "nyholm/psr7-server": ">=1.0",
        "laminas/laminas-diactoros": ">=2.5",
        "php-di/php-di": ">=6.4",
        "brainstormdevel/basepath": ">=2.0",
        "slim/twig-view": ">=3.0",
        "catfan/medoo": ">=2.1",
        "composer/ca-bundle": ">=1.2",
        "odan/twig-assets": ">=3.2",
        "odan/session": ">=5.1",
        "monolog/monolog": ">=2.3",
        "ext-openssl": ">=7.2",
        "robmorgan/phinx": ">=0.13",
        "per-seo/locale": ">=1.0",
        "per-seo/gzip": ">=1.0",
        "per-seo/language": ">=1.0",
        "per-seo/forwarded-proto": ">=1.0",
		"per-seo/httphtmlexception": ">=1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": ">=3",
        "phpstan/phpstan": ">=1.0",
        "phpunit/phpunit": ">=10.5",
        "selective/test-traits": ">=4",
        "squizlabs/php_codesniffer": ">=3"
    },
	"autoload": {
        "psr-4": {
		"PerSeo\\": "core/perseo",
		"Modules\\": "modules/"
        }
    },
	"config": {
		"optimize-autoloader": true
	}
}
